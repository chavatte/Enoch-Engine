{
  "name": "enoch-engine",
  "version": "1.0.0",
  "description": "The Arcane Node.js API Foundation. Robust, Secure, and Ready for the Unknown.",
  "author": "[DevChavatte] - Jo√£o Carlos Chavatte",
  "license": "ISC",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "test": "vitest",
    "test:ui": "vitest --ui --api 9500",
    "report": "cross-env NODE_ENV=development vite preview --outDir report/HTML",
    "db:migrate": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "db:migrate:to": "cross-env NODE_ENV=development sequelize-cli db:migrate --to",
    "db:migrate:undo": "cross-env NODE_ENV=development sequelize-cli db:migrate:undo",
    "db:migrate:undo:to": "cross-env NODE_ENV=development sequelize-cli db:migrate:undo:all --to",
    "db:migration:create": "cross-env NODE_ENV=development node scripts/create-migration.cjs",
    "usage": "cross-env NODE_ENV=development node scripts/help.cjs",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs}": [
      "vitest related --run"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.999.0",
    "@dotenvx/dotenvx": "^1.52.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "file-type": "^21.3.0",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^8.0.1",
    "pg": "^8.19.0",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.18",
    "chalk": "^5.6.2",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.14",
    "sequelize-cli": "^6.6.5",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
